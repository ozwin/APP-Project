<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="500" prefWidth="1000.0"
      spacing="10" xmlns="http://javafx.com/javafx/11.0.14-internal"
      fx:controller="com.app.project.javafxcontroller.TenantController">
    <children>
        <ListView fx:id="tenantListView" prefHeight="500.0" prefWidth="300.0"/>
        <Button onAction="#handleAddNewTenant" text="Add New Tenant"/>
        <Button fx:id="closebtn" onAction="#back" text="Back"/>
    </children>
    <stylesheets>
        <!--    <URL value="@style.css" />-->
    </stylesheets>
</VBox>
